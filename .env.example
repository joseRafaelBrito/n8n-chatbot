# n8n Environment Variables
# Copy this file to .env and update the values

# Basic Authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme

# Encryption Key (generate a secure random string)
N8N_ENCRYPTION_KEY=replace_with_a_secret_key

# Webhook URL (update with your actual domain for production)
WEBHOOK_URL=http://localhost:5678

# Additional n8n Configuration (optional)
N8N_HOST=0.0.0.0
N8N_PORT=5678
GENERIC_TIMEZONE=America/Santo_Domingo
